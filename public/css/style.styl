  #name
    margin-bottom 0.313em
  #name2, #address, #phone, #fax
    clear both
  #address
    margin-bottom 0.313em
  #contact
    padding-left 0.625em
  #address, #email, #phone
    .logo
      color #666
      float left
      clear both
