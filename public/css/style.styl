#footer-container
  padding 0.625em 0 0.625em 0
  background-color #f7f7f7 !important

footer.doc
  width 50.000em
  padding-left 0

footer
  width 60.156em
  margin 0 auto 0 auto
  padding 0.391em 0 1.563em 2.344em
  font-size 0.8em
  color #9A9EA8
  p
    float left
  .logo
    color #bbb
    float right
    font-family 'arthrolife'
