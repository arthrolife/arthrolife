html,body,div,span,applet,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,a,
abbr,acronym,address,big,cite,code,del,dfn,em,img,ins,kbd,q,s,samp,small,
strike,strong,sub,sup,tt,var,b,u,i,center,dl,dt,dd,ol,ul,li,fieldset,form,label,
legend,table,caption,tbody,tfoot,thead,tr,th,td,article,aside,canvas,details,
embed,figure,figcaption,footer,header,hgroup,menu,nav,output,ruby,section,time,
mark,summary,audio,video
  margin 0
  padding 0
  border 0
  font-size 100%
  font inherit
  vertical-align baseline
article,aside,details,figcaption,figure,footer,header,hgroup,menu,nav,section
  display block
body
  line-height 1
ol,ul
  list-style none
blockquote,q
  quotes none
blockquote:before,blockquote:after,q:before,q:after
  content none
table
  border-collapse collapse
  border-spacing 0

@font-face
  font-family 'arthrolife'
  src url('../fonts/arthrolife.eot')
  src url('../fonts/arthrolife.eot?#iefix') format('embedded-opentype'),url('../fonts/arthrolife.woff') format('woff'),url('../fonts/arthrolife.ttf') format('truetype')
  font-weight normal
  font-style normal

html
  -webkit-font-smoothing antialiased
  overflow-x hidden

body
  cursor default

body, textarea
  font normal normal normal 16px 'Lucida Grande', 'Helvetica Neue', 'Geneva', sans-serif

.doc
  font-size 14px

#about-container
  padding 10px 0 10px 0
  background-color #ddd

#header-container
  background-color black
  width 100%
  height 250px
  border-bottom 1px solid #000

header
  width 800px
  height 100%
  margin 0 auto 0 auto
  background-image url('../img/knee-xray.jpg')
  background-repeat no-repeat
  -webkit-transition opacity 2s ease-in
  h1
    float right
    font-size 3em
    font-family 'Nunito'
    color #fff
  h2
    clear both
    float right
    padding 90px 70px 0 0
    font-size 1.5em
    color #fff
  .logo
    float left
    padding-top 2px
    color #fff
    font-family 'arthrolife'
    font-size 4em
  ul
    clear both
    float right
    margin-top -12px
    li
      float left
      padding-left 10px
      text-align right
      a
        text-decoration none
      a:link, a:visited
        color #999
      a:hover
        color #fff
.foot
  opacity 0
  background-image url('../img/foot-xray.jpg')
  background-position 160px 0
.hand
  background-image url('../img/hand-xray.jpg')

#header-container.doc
  height 50px
  border-bottom none
  background-color #ddd

header.doc
  height 100%
  background-image none
  .logo
    font-size 3em
  h1
    margin-top -2px

.section-heading
  padding 10px 0 10px 0
  background-color #f7f7f7 !important
  h1
    width 800px
    margin 0 auto 0 auto
    padding-left 50px
    color #444
    font-size 1.3em

#footer-container
  padding 10px 0 10px 0
  background-color #f7f7f7 !important

footer.doc
  width 800px
  padding-left 0

footer
  width 770px
  margin 0 auto 0 auto
  padding 5px 0 20px 30px
  font-size 0.8em
  color #9A9EA8
  p
    float left
  .logo
    color #bbb
    float right
    font-family 'arthrolife'
    font-size 1.3em

section.doc
  width 800px
  padding-left 0

section
  overflow-y auto
  overflow-x hidden
  width 770px
  margin 0 auto 0 auto
  padding 10px 0 10px 20px
  clear both
  h1
    font-size 1.3em
    margin-bottom 5px
    border-bottom 2px dotted #eee
  h2
    margin-left 15px
    font-size 1.15em
    border-bottom 1px solid #eee
  p
    float left
    margin 3px 0 0 15px
    font-size 1em
  p.doc
    margin 10px 0 10px 15px
  ul
    clear both
    padding 5px 0 10px 40px
    list-style disc
  li
    clear both
    margin-bottom 5px
  .col
    float left
    width 46%
    h2
      font-size 1em
      border-bottom 1px solid rgba(0,0,0,0.1)
      padding-bottom 2px
      margin-bottom 5px
      a
        font-size 0.8em
        float right
    p
      float left
      font-size 0.8em
    a:link, a:visited
        color rgb(12,131,235)

  .col-right
    float right
    margin-right 0
  .col-right iframe
    margin-left 15px
    border-radius 6px
  #name
    margin-bottom 5px
  #name2, #address, #phone, #fax
    clear both
  #address
    margin-bottom 5px
  #contact
    padding-left 10px
  #address, #email, #phone
    .logo
      color #666
      float left
      clear both
      margin-top 3px
      padding-left 15px
      width 1.3em
      font-family 'arthrolife'
  #fax
    .logo
      color #333
      margin-top 3px
      font-size 0.8em
      float left
      clear both
      width 1.6em
      padding-left 15px

#research
  padding 10px 0 0 15px
  .col
    margin-left 0
  p
    margin-top 0
    font-size 0.8em
  .research-contact
    margin-top  10px
  ul
    clear both
    padding-top 5px
    list-style-type disc
  li
    font-size 0.8em
    margin-bottom 5px
  li:last-child
    margin-bottom 0
  a:link, a:visited
    color rgb(12,131,235)

#about
  width 800px
  margin 0 auto 0 auto
  padding 10px 0 10px 0
  clear both
  overflow auto
  p
    float left
    width 95%
    margin 0 0 0 0.35em
    font-size 1.3em
    color #888
  .logo
    float left
    margin-top 2px
    color #fff
    font-family 'arthrolife'
    font-size 1.1em

#ask
  position fixed
  height 300px
  right -300px
  top 105px
  transition right 2s
  -moz-transition right 2s
  -webkit-transition right 2s

.ask-show
  right 10px !important

#ask-form
  border-radius 0 6px 6px 6px
  float left
  width 300px
  padding-top 3px
  background-color rgb(38,168,255)
  background-image linear-gradient(bottom, rgb(12,131,235) 19%, rgb(38,168,255) 60%)
  background-image -o-linear-gradient(bottom, rgb(12,131,235) 19%, rgb(38,168,255) 60%)
  background-image -moz-linear-gradient(bottom, rgb(12,131,235) 19%, rgb(38,168,255) 60%)
  background-image -webkit-linear-gradient(bottom, rgb(12,131,235) 19%, rgb(38,168,255) 60%)
  background-image -ms-linear-gradient(bottom, rgb(12,131,235) 19%, rgb(38,168,255) 60%)
  p
    padding 10px
    color #fff
  input[type="text"]
    font-size 0.8em
    width 268px
    padding 5px
    margin-left 10px
    border 1px solid lightsteelblue
    border-radius 4px
  textarea
    width 268px
    padding 5px
    margin-left 10px
    border 1px solid lightsteelblue
    border-radius 4px
    resize none
    font-size 0.8em
  #ask-submit
    float right
    border-radius 4px
    color #fff
    border 1px solid lightsteelblue
    margin 5px 10px 10px 10px
    padding 5px 8px 5px 8px

#ask-button
  padding 10px
  border-radius 6px 0 0 6px
  float left
  width 120px
  height 1.45em
  background-color rgb(38,168,255)
  .logo
    float left
    margin -1px 0 0 1px
    color #fff
    font-family 'arthrolife'
    font-size 1.3em
  p
    float left
    padding 3px 0 0 8px
    font-size 1em
    color #fff

