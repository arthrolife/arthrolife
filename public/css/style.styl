html,body,div,span,applet,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,a,
abbr,acronym,address,big,cite,code,del,dfn,em,img,ins,kbd,q,s,samp,small,
strike,strong,sub,sup,tt,var,b,u,i,center,dl,dt,dd,ol,ul,li,fieldset,form,label,
legend,table,caption,tbody,tfoot,thead,tr,th,td,article,aside,canvas,details,
embed,figure,figcaption,footer,header,hgroup,menu,nav,output,ruby,section,time,
mark,summary,audio,video
  margin 0
  padding 0
  border 0
  font-size 100%
  font inherit
  vertical-align baseline
article,aside,details,figcaption,figure,footer,header,hgroup,menu,nav,section
  display block
body
  line-height 1
ol,ul
  list-style none
blockquote,q
  quotes none
blockquote:before,blockquote:after,q:before,q:after
  content none
table
  border-collapse collapse
  border-spacing 0

@font-face
  font-family 'arthrolife'
  src url('../fonts/arthrolife.eot')
  src url('../fonts/arthrolife.eot?#iefix') format('embedded-opentype'),url('../fonts/arthrolife.woff') format('woff'),url('../fonts/arthrolife.ttf') format('truetype')
  font-weight normal
  font-style normal

html
  -webkit-font-smoothing antialiased
  overflow-x hidden

body
  cursor default

body, textarea
  font normal normal normal 16px 'Lucida Grande', 'Helvetica Neue', 'Geneva', sans-serif

.doc
  font-size 0.875em

#about-container
  padding 0.625em 0 0.625em 0
  background-color #ddd

#header-container
  background-color black
  width 100%
  height 15.625em
  border-bottom 0.063em solid #000

header
  width 50.000em
  height 100%
  margin 0 auto 0 auto
  background-image url('../img/knee-xray.jpg')
  background-repeat no-repeat
  -webkit-transition opacity 2s ease-in
  h1
    float right
    font-size 3em
    font-family 'Nunito'
    color #fff
  h2
    clear both
    float right
    padding 3.750em 2.917em 0 0
    font-size 1.5em
    color #fff
  .logo
    float left
    padding-top 0.025em
    color #fff
    font-family 'arthrolife'
    font-size 4em
  ul
    clear both
    float right
    margin-top -0.750em
    li
      float left
      padding-left 0.625em
      text-align right
      a
        text-decoration none
      a:link, a:visited
        color #999
      a:hover
        color #fff

#header-container.doc
  height 3.571em
  padding-top 0.4em
  border-bottom none
  background-color #ddd

header.doc
  width 57.143em
  height 100%
  background-image none
