#ask
  z-index 99
  position fixed
  height 18.750em
  right -18.750em
  top 6.563em
  transition right 2s
  -moz-transition right 2s
  -webkit-transition right 2s

.ask-show
  right 0.625em !important

#ask-form
  border-radius 0 0.375em 0.375em 0.375em
  float left
  width 18.750em
  padding-top 0.188em
  background-color rgb(38,168,255)
  background-image linear-gradient(bottom, rgb(12,131,235) 19%, rgb(38,168,255) 60%)
  background-image -o-linear-gradient(bottom, rgb(12,131,235) 19%, rgb(38,168,255) 60%)
  background-image -moz-linear-gradient(bottom, rgb(12,131,235) 19%, rgb(38,168,255) 60%)
  background-image -webkit-linear-gradient(bottom, rgb(12,131,235) 19%, rgb(38,168,255) 60%)
  background-image -ms-linear-gradient(bottom, rgb(12,131,235) 19%, rgb(38,168,255) 60%)
  p
    padding 0.625em
    color #fff
  input[type="text"]
    font-size 0.8em
    width 20.938em
    padding 0.391em
    margin-left 0.781em
    border 0.078em solid lightsteelblue
    border-radius 0.313em
  textarea
    width 20.938em
    padding 0.391em
    margin-left 0.781em
    border 0.078em solid lightsteelblue
    border-radius 0.250em
    border-radius 0.313em
    resize none
    font-size 0.8em
  #ask-submit
    float right
    border-radius 0.250em
    color #fff
    border 0.063em solid lightsteelblue
    margin 0.313em 0.625em 0.625em 0.625em
    padding 0.313em 0.500em 0.313em 0.500em

#ask-button
  padding 0.625em
  border-radius 0.375em 0 0 0.375em
  float left
  width 7.500em
  height 1.45em
  background-color rgb(38,168,255)
  .logo
    float left
    margin -0.063em 0 0 0.063em
    color #fff
    font-family 'arthrolife'
    font-size 1.3em
  p
    float left
    padding 0.188em 0 0 0.500em
    font-size 1em
    color #fff

