section.doc
  width 50.000em
  padding-left 0

section
  overflow-y auto
  overflow-x hidden
  width 48.125em
  margin 0 auto 0 auto
  padding 0.625em 0 0.625em 1.250em
  clear both
  h1
    font-size 1.3em
